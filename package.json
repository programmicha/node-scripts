  "dependencies": {
    "async": "0.2.x",
    "cheerio": "0.13.x",
    "data2xml": "0.9.x",
    "request": "2.30.x"
  },
  "devDependencies": {
    "mocha": "1.10.x",
    "should": "1.2.x"
  },
  "engines": {
    "node": ">=0.10.10"
  }
}